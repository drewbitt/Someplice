{
	"name": "someplice",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:codegen": "kysely-codegen --out-file ./src/lib/types/data.d.ts",
		"db:migrate": "vite-node ./src/lib/db/migrate-to-latest.ts"
	},
	"dependencies": {
		"@svelteuidev/composables": "^0.9.0",
		"@svelteuidev/core": "^0.9.0",
		"@trpc/client": "^10.9.1",
		"@trpc/server": "^10.9.1",
		"better-sqlite3": "^8.0.1",
		"kysely": "^0.23.4",
		"trpc-sveltekit": "^3.2.5",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/kit": "^1.3.6",
		"@types/better-sqlite3": "^7.6.3",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"autoprefixer": "^10.4.13",
		"daisyui": "^2.49.0",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"kysely-codegen": "^0.9.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"radix-icons-svelte": "^1.2.1",
		"svelte": "^3.55.1",
		"svelte-check": "^2.10.3",
		"svelte-dnd-action": "^0.9.22",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.0.4",
		"vite-node": "^0.26.3",
		"vitest": "^0.25.8"
	},
	"type": "module"
}
