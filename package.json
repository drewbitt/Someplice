{
	"name": "someplice",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:codegen": "kysely-codegen --out-file ./src/lib/types/data.d.ts",
		"db:migrate": "vite-node ./src/lib/db/migrate-to-latest.ts"
	},
	"dependencies": {
		"@svelteuidev/composables": "^0.10.0",
		"@svelteuidev/core": "^0.10.0",
		"@trpc/client": "^10.12.0",
		"@trpc/server": "^10.12.0",
		"better-sqlite3": "^8.1.0",
		"kysely": "^0.23.4",
		"trpc-sveltekit": "^3.3.1",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.1.63",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.8.3",
		"@types/better-sqlite3": "^7.6.3",
		"@types/node": "^18.14.0",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"autoprefixer": "^10.4.13",
		"daisyui": "^2.50.2",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"kysely-codegen": "^0.9.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-dnd-action": "^0.9.22",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"unplugin-icons": "^0.15.3",
		"vite": "^4.1.3",
		"vite-node": "^0.28.5",
		"vitest": "^0.28.5"
