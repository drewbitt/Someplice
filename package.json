{
	"name": "someplice",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:codegen": "kysely-codegen --out-file ./src/lib/types/data.d.ts && prettier --plugin-search-dir . --write ./src/lib/types/data.d.ts",
		"db:migrate": "vite-node ./src/lib/db/migrate-to-latest.ts",
		"db:migrate:single": "vite-node ./src/lib/db/migrate-to-latest.ts --migration"
	},
	"dependencies": {
		"@svelteuidev/composables": "^0.10.0",
		"@svelteuidev/core": "^0.10.0",
		"@trpc/client": "^10.20.0",
		"@trpc/server": "^10.20.0",
		"better-sqlite3": "^8.3.0",
		"kysely": "^0.24.2",
		"trpc-sveltekit": "^3.5.5",
		"vite-node": "^0.29.8",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.1.79",
		"@playwright/test": "^1.32.3",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.15.7",
		"@types/better-sqlite3": "^7.6.4",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vite-pwa/sveltekit": "^0.2.1",
		"autoprefixer": "^10.4.14",
		"daisyui": "^2.51.5",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"kysely-codegen": "^0.10.0",
		"postcss": "^8.4.22",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-dnd-action": "^0.9.22",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"unplugin-icons": "^0.16.1",
		"vite": "^4.2.2",
		"vite-plugin-pwa": "^0.14.7",
		"vitest": "^0.29.8"
	},
	"type": "module"
}
